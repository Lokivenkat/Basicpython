#string formatting techniques
name="Loki venkat"
print("hello %s how are you"%(name))
age=18
print("your age is %d"%age)
print("octal version of your age is %o"%age)
print("hexadecimal version of your age is %x"%age)
print("hexadecimal version of your age is %X"%age)
print("float version is %f"%age)

#hello Loki venkat how are you
#your age is 18
#octal version of your age is 22
#hexadecimal version of your age is 12
#hexadecimal version of your age is 12
#float version is 18.000000
